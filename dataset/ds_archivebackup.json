{
	"name": "ds_archivebackup",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().FileName,utcnow(),'.csv')",
					"type": "Expression"
				},
				"container": "archivebackup"
			},
			"columnDelimiter": ",",
			"compressionCodec": "tar",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	}
}